<launch>

    <arg name="project" default="lsm"/>
    <rosparam command="load" file="$(find lsm)/config/run_doals.yaml" />
    <node pkg="$(arg project)" type="data_publisher_node" name="data_publisher_node" output="screen" />
    <node pkg="$(arg project)" type="static_mapping_node" name="static_mapping_node" output="screen" />
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find lsm)/rviz/run_doals.rviz" />

</launch>
